{
  "manifest_version": 3,
  "name": "Yours",
  "version": "2.1",
  "description": "Accessing your social self",
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "tabs",
    "notifications"
  ],
  "icons": {
    "16": "imgs2/person16.ico",
    "32": "imgs2/person32.ico",
    "48": "imgs2/person48.ico",
    "128": "imgs2/person128.ico"
  },
  "action": {
    "default_popup": "pop.html",
    "default_icon": "imgs2/person16.ico"
  },
  "background": {
    "service_worker": "modules/background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
